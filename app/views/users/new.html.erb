<h1>Sign up</h1>

<%= form_for :user, url: '/users' do |f| %>

  First_name: <%= f.text_field :first_name %></br>
  Last_name: <%= f.text_field :last_name %></br>
  Email: <%= f.text_field :email %></br>
  Password: <%= f.password_field :password %></br>
  Passwrod Confirmation: <%= f.password_field :password_confirmation %></br>
  <%= f.submit "Submit" %>

<% end %>